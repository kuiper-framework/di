{
    "name": "kuiper/di",
    "type": "library",
    "description": "Dependency injection component",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "container-interop/container-interop": "^1.1",
        "psr/log": "^1.0",
        "psr/cache": "^1.0",
        "symfony/event-dispatcher": "^3.2"
    },
    "suggest": {
        "ocramius/proxy-manager": "for lazy object injection",
        "kuiper/helper": "for dot array config",
        "kuiper/cache": "for cache annotations",
        "kuiper/annotations": "for annotations"
    },
    "require-dev": {
        "ocramius/proxy-manager": "^1.0",
        "phpunit/phpunit": "^5.0",
        "kuiper/annotations": "^0.1",
        "kuiper/helper": "^0.1",
        "winwin/composer-phpcs-plugin": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\di\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\di\\": "tests/"
        }
    }
}
